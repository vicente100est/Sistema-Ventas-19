	<queryString>
		<![CDATA[select  r.cod_cliente,
r.cod_remito,
r.cod_empleado,
r.fecha,
rr.referencia,
rr.cantidad,
rr.valor_total,
rr.valor_unitario,
rr.Total,
c.nombres,
c.apellidos,
e.apellidos as apellido,
e.nombres as nombre

from remito r
inner join referencia_remito rr on r.cod_remito=rr.cod_remito
inner join cliente c on r.cod_cliente=c.cod_cliente
inner join empleado e on r.cod_empleado=e.cod_empleado

where r.cod_remito = $P{codr}]]>
	</queryString>
